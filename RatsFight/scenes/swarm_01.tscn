resource/name = "swarm_01"
script/source = "extends Node

signal signal_clear

export var camera_limit = 0

var _nb_enemies
var _count_dead

func _ready():
	_count_dead = 0
	_nb_enemies = get_children().size()
	for child in get_children():
		child.connect(\"signal_dead\", self, \"one_dead\")

func one_dead():
	_count_dead += 1
	if _count_dead == _nb_enemies:
		emit_signal(\"signal_clear\")
		queue_free()
		
func get_camera_limit():
	return camera_limit"

